{
    "name": "ts-monorepo",
    "version": "0.9.0",
    "description": "Monorepo template for typescript project",
    "type": "module",
    "private": true,
    "workspaces": [
        "apps/**",
        "packages/**"
    ],
    "keywords": [
        "monorepo",
        "typescript",
        "eslint",
        "prettier",
        "jest",
        "pnpm"
    ],
    "license": "MIT",
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "format": "prettier --write \"packages/**/*.ts\" \"apps/**/*.ts\""
    },
    "devDependencies": {
        "prettier": "^2.8.7",
        "turbo": "^1.9.1"
    },
    "packageManager": "yarn@3.5.0"
}
